/* CSS Document */
body,td,th {font-size: 12px;color: #25B0B1; line-height:20px; font-family:"ו";}
body {background:#000;	margin: 0px;}
img{ border:0;}
input,ul,li,p,form{ padding:0;margin:0;font-size:12px;}
h5,h4,h3,h2,h1{padding:0;margin:0;font-size:12px; font-weight:100;}
ul,li{list-style:none;}
a:link,a:visited{color:#25B0B1;text-decoration:none;}
a:hover{color:#D30902; text-decoration: underline;}
.red{color:#FA0F03}
.red a:link, .red a:visited{color:#FA0F03;}
.red a:hover{color:#f30;}
.red1{color:#ff0000}
.bai{color:#fff;}
.bai a:link, .bai a:visited{color:#fff;}
.bai a:hover{color:#ff0;}


/* top */
#top_w{width:100%; background:url(http://static.kunlun.com/web/qjp/images/qjp_01.jpg) no-repeat center bottom;}
#top_navw{width:100%;background:url(http://static.kunlun.com/web/qjp/images/top.gif) no-repeat center top;text-align:left; height:auto; overflow:hidden;}
#top_navw .top_nav{width:1000px;text-align:left;margin:auto;position:relative; top:0; left:0;}

#top_w .nav{padding:55px 0 0 108px; height:100px; position:absolute;z-index:1000; top:0; left:0;}
#top_w .nav .navlist{width:460px;height:23px; text-align:center;clear:both;display:none; margin:auto;}
#top_w .nav .navlist a{float:left;color:#fff;width:77px;background:url(http://static.kunlun.com/web/qjp/images/2_nav1.gif) no-repeat 0 0;cursor:pointer; display:block; line-height:23px;}
#top_w .nav .navlist a:hover{width:77px;color:#ff0;background:url(http://static.kunlun.com/web/qjp/images/2_nav1.gif) no-repeat 0 0;cursor:pointer;text-decoration:none;}
#top_w .nav ul{clear:both;height:35px; text-align:left;}
#top_w .nav li{float:left;width:89px;height:32px; overflow:hidden;}
#top_w .nav .nav_down{background:url(http://static.kunlun.com/web/qjp/images/nav.gif) no-repeat;text-decoration:none;}
#top_w .nav li img{ width:89px; height:32px; border:0; overflow:hidden}

